// localstack/terraform.tfvars
runtime                  = "python3.12"
memory_size              = 256
memory_size_ip           = 256
memory_size_slack        = 256
allowed_regions          = "['us-east-1', 'us-east-2', 'us-west-1', 'us-west-2']"
ip_address               = true
org_primary_account      = "123456789012"
environment              = "prd"
security_audit_role_name = "domain-protect-audit-role"
slack_channels           = ["domain-protect-alerts"]
